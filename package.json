{
  "name": "jarvis",
  "version": "0.0.1",
  "description": "AI",
  "type": "module",
  "engines": {
    "node": ">=18.17.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "jest --coverage",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "langchain": "^0.0.146",
    "tslib": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErosZZH/jarvis.git"
  },
  "keywords": [
    "AI"
  ],
  "author": "Rick Zhu",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ErosZZH/jarvis/issues"
  },
  "homepage": "https://github.com/ErosZZH/jarvis#readme",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-api-utils": "^1.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
